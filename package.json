{
   "name": "crystallize-frontend-boilerplate",
   "version": "2.7.1",
   "license": "MIT",
   "engines": {
      "node": ">=8.10"
   },
   "repository": {
      "url": "git@github.com:CrystallizeAPI/crystallize-frontend-boilerplate.git",
      "type": "git"
   },
   "scripts": {
      "dev": "node server/index.js",
      "dev:inspect": "node --inspect-brk server/index.js",
      "build": "next build",
      "start": "NODE_ENV=production node server/index.js",
      "prod": "npm run build && npm run start",
      "test": "NODE_ENV=test jest",
      "lint": "eslint .",
      "validate": "npm run test && lint-staged",
      "greet": "node .crystallize-greeting/",
      "postinstall": "npm run greet"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run validate"
      }
   },
   "lint-staged": {
      "*.js": [
         "prettier --write",
         "eslint --fix",
         "git add"
      ]
   },
   "dependencies": {
      "@babel/core": "^7.1.2",
      "@crystallize/content-chunk": "^3.3.0",
      "@crystallize/node-klarna": "^2.1.1",
      "@crystallize/react-basket": "^6.0.0-beta.3",
      "@crystallize/react-dialog": "^4.2.1",
      "@crystallize/react-image": "^2.2.0",
      "@crystallize/react-layout": "^3.0.0",
      "apollo-boost": "^0.1.28",
      "apollo-cache-inmemory": "^1.5.1",
      "apollo-client": "^2.4.5",
      "babel-plugin-module-resolver": "^3.2.0",
      "babel-plugin-styled-components": "^1.10.0",
      "body-parser": "^1.18.3",
      "change-case": "^3.0.2",
      "cross-fetch": "^2.2.3",
      "dotenv": "^6.1.0",
      "express": "^4.16.4",
      "graphql": "^14.0.2",
      "graphql-tag": "^2.10.0",
      "helmet": "^3.14.0",
      "i18next": "^11.7.0",
      "isarray": "^2.0.4",
      "jsonwebtoken": "^8.5.1",
      "lodash.merge": "^4.6.1",
      "mjml": "^4.4.0-beta.1",
      "next": "^7.0.3",
      "ow": "^0.8.0",
      "polished": "^2.3.0",
      "prop-types": "^15.7.2",
      "react": "^16.8.2",
      "react-apollo": "^2.2.4",
      "react-dom": "^16.8.2",
      "react-i18next": "^7.11.1",
      "styled-components": "^4.1.3",
      "styled-is": "^1.1.5",
      "upper-case-first": "^1.1.2"
   },
   "devDependencies": {
      "babel-core": "7.0.0-bridge.0",
      "babel-eslint": "^10.0.1",
      "babel-jest": "^23.6.0",
      "chalk": "^2.4.1",
      "enzyme": "^3.7.0",
      "enzyme-adapter-react-16": "^1.6.0",
      "eslint": "^5.8.0",
      "eslint-config-airbnb": "^17.1.0",
      "eslint-config-prettier": "^3.1.0",
      "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
      "eslint-plugin-import": "^2.14.0",
      "eslint-plugin-jsx-a11y": "^6.1.2",
      "eslint-plugin-prettier": "^3.0.0",
      "eslint-plugin-react": "^7.11.1",
      "husky": "^1.1.2",
      "inquirer": "^6.2.0",
      "jest": "^23.6.0",
      "lint-staged": "^8.0.4",
      "node-emoji": "^1.8.1",
      "prettier": "^1.14.3",
      "supports-color": "^5.5.0"
   }
}
